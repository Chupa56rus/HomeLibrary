<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="125.0" prefWidth="277.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.ExitWindowController">
   <children>
      <Label layoutX="17.0" layoutY="27.0" text="Are you sure you want to cancel?">
         <font>
            <Font name="Bookman Old Style" size="15.0" />
         </font></Label>
      <Button fx:id="exitWindowYesButton" layoutX="51.0" layoutY="81.0" mnemonicParsing="false" onAction="#exitWindowYesButtonAction" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: #861209;" text="Yes" textFill="WHITE">
         <font>
            <Font name="Rockwell" size="15.0" />
         </font></Button>
      <Button fx:id="exitWindowNoButton" layoutX="146.0" layoutY="81.0" mnemonicParsing="false" onAction="#exitWindowNoButtonAction" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: #861209;" text="No" textFill="WHITE">
         <font>
            <Font name="Rockwell" size="15.0" />
         </font></Button>
   </children>
</AnchorPane>
